class VisitorForm:
    def __init__(self):
        self._name = None
        self._aboutLW = "We provide training in holistic approach."
        self._phone = None
        self._email = None
        self._age = None
        self._wifipass = "mypass"
        self._purpose = None

    def getWifiPass(self):
        return self._wifipass

    def setAge(self, myage):
        if myage >= 0:
            self._age = myage
        else:
            print("Negative age is not supported.")

    def getAge(self):
        return self._age

    def __str__(self):
        return (f"Name: {self._name}\n"
                f"About LW: {self._aboutLW}\n"
                f"Phone: {self._phone}\n"
                f"Email: {self._email}\n"
                f"Age: {self._age}\n"
                f"WiFi Pass: {self._wifipass}\n"
                f"Purpose: {self._purpose}")
